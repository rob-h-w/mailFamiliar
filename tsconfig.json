{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowJs": true,
    "baseUrl": "./src",
    "checkJs": true,
    "lib": ["es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "es6"
  }
}
